async function getData() {
    try {
        const response = await fetch('api link');
        
        if (!response.ok) {
            throw new Error(`error status: ${response.status}`);
        }

        const data = await response.json();

        console.log(data);

        return data;
    } catch (error) {
        console.error("Something went wrong:", error.message);
        throw error;
    }
}